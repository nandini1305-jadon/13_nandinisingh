import java.util.Scanner;

public class NonRepeatedDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number: ");
        int num = sc.nextInt();

        int[] freq = new int[10];

        int temp = num;
        while (temp != 0) {
            freq[temp % 10]++;
            temp /= 10;
        }

        int count = 0;
        for (int f : freq) {
            if (f == 1) count++;
        }

        System.out.println("Non-repeated digits: " + count);
    }
}
