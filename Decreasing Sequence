import java.util.Scanner;

public class DecreasingSequence {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter sequence length: ");
        int n = sc.nextInt();
        int[] arr = new int[n];

        System.out.println("Enter sequence elements:");
        for (int i = 0; i < n; i++) arr[i] = sc.nextInt();

        boolean decreasing = true;
        for (int i = 1; i < n; i++) {
            if (arr[i] >= arr[i - 1]) {
                decreasing = false;
                break;
            }
        }

        if (decreasing)
            System.out.println("Sequence is strictly decreasing");
        else
            System.out.println("Sequence is not strictly decreasing");
    }
}
