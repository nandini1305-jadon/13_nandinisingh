import java.util.Scanner;

public class CountOdd {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int odd = 0;

        for (int i = 1; i <= 5; i++) {
            System.out.print("Enter number " + i + ": ");
            int num = sc.nextInt();
            if (num % 2 != 0)
                odd++;
        }

        System.out.println("Count of odd numbers: " + odd);
    }
}
